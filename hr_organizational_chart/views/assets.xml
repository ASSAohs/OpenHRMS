<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="Org Chart assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script src="/hr_organizational_chart/static/js/hr_org_chart.js"></script>
            <script src="/hr_organizational_chart/static/js/jquery_hr_orgchart.js"></script>
        </xpath>
    </template>
    <template id="assets_backend_css" inherit_id="web.assets_backend">
        <xpath expr="//link" position="before">
            <link type="text/css" href="/hr_organizational_chart/static/src/css/hr_orgchart.css" rel="stylesheet"/>
            <link type="text/css" href="/hr_organizational_chart/static/src/css/style.css" rel="stylesheet"/>
        </xpath>
    </template>
</odoo>